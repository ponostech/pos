<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<fx:root id="AnchorPane" prefHeight="503.0" prefWidth="791.0" type="AnchorPane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView fx:id="supplierTable" layoutX="14.0" layoutY="174.0" prefHeight="298.0" prefWidth="764.0">
        <columns>
          <TableColumn fx:id="idCol" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="nameCol" prefWidth="119.0" text="NAME" />
            <TableColumn fx:id="contactCol" prefWidth="155.0" text="CONTACT" />
            <TableColumn fx:id="actionCol" prefWidth="299.0" text="ACTIONS" />
        </columns>
      </TableView>
      <JFXButton fx:id="newSupplierBtn" buttonType="RAISED" layoutX="23.0" layoutY="135.0" onAction="#onNewSupplierBtnClick" text="New Supplier">
         <graphic>
            <FontAwesomeIconView glyphName="USER_PLUS" />
         </graphic>
      </JFXButton>
      <TextField fx:id="searchField" layoutX="406.0" layoutY="135.0" prefHeight="25.0" prefWidth="283.0" />
      <Button fx:id="searchBtn" layoutX="689.0" layoutY="135.0" mnemonicParsing="false" text="Search">
         <graphic>
            <FontAwesomeIconView glyphName="SEARCH" />
         </graphic>
      </Button>
   </children>
</fx:root>
