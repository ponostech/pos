<?import javafx.scene.layout.AnchorPane?>

<fx:root id="AnchorPane" prefHeight="402.0" prefWidth="733.0" type="AnchorPane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <JFXButton fx:id="newStoreBtn" layoutX="21.0" layoutY="39.0" onAction="#onNewStoreBtnClick" prefHeight="25.0" prefWidth="104.0" text="New Store">
