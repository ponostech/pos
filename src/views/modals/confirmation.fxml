<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane id="AnchorPane" prefHeight="194.0" prefWidth="481.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane layoutX="-2.0" prefHeight="67.0" prefWidth="490.0" styleClass="top-pane">
         <children>
            <Label fx:id="title" layoutX="36.0" layoutY="28.0" styleClass="h2" text="Label" />
            <FontAwesomeIconView fx:id="close" fill="#e31a07" glyphName="CLOSE" layoutX="458.0" layoutY="26.0" size="28" />
